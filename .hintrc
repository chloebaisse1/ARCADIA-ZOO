{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust",
          "text-align: -webkit-match-parent",
          "-webkit-overflow-scrolling",
          "overflow-anchor"
        ]
      }
    ],
    "css-prefix-order": "off",
    "no-inline-styles": "off",
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 124",
    "not firefox <= 125",
    "not safari <= 17.4"
  ]
}